{
	"manifest_version": 2,
		"name": "LeetDown",
		"version": "0.1",
		"minimum_chrome_version": "23",
		"icons": {
			"16": "icon_16.png",
			"128": "icon_128.png"
		},
        "browser_action":{
            "default_popup":"popup.html"
        }
        ,
        "background": {
            "scripts": ["main.js"]
        },
		"content_scripts":[{
            "matches":["https://leetcode.com/problems/*"],
            "run_at":"document_idle",
            "js":["jquery.js","downloader.js"]
        }]
}
